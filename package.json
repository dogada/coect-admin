{
  "name": "coect-admin",
  "version": "0.1.0",
  "description": "Admin area for coect web-sites.",
  "main": "server/index.js",
  "browser": "client/index.js",
  "browserify": {
    "transform": [
      "riotify",
      "babelify",
      "browserify-versionify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {},
  "scripts": {
    "test": "npm run test:client",
    "lint": "jshint server/ client/",
    "validate": "npm ls",
    "check": "npm run lint && npm run test"
  },
  "keywords": [
    "coect",
    "admin"
  ],
  "author": "Dmytro V. Dogadailo <dvd@dogada.org>",
  "license": "GPL-2.0",
  "dependencies": {
    "debug": "~2.2.0",
    "tflow": "^0.4.1"
  },
  "peerDependencies": {
    "wpml": "0.x",
    "coect": "0.x",
    "riot": "2.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dogada/coect-admin.git"
  },
  "bugs": {
    "url": "https://github.com/dogada/coect-admin/issues"
  },
  "homepage": "http://www.coect.net/",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "riot": "2.x",
    "riotify": "~1.0.0"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ]
}
